{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/CreateOrder",
        "function": "CreateOrder"
      },
      {
        "source": "/api/Report",
        "function": "Report"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
